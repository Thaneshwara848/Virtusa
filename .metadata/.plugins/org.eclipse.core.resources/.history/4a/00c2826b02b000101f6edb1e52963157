package comB;
class A
{
	A(){
		System.out.println("Hi A class ");
	}
}

class B extends A
{
	B(){
		System.out.println("Hi B class ");
	}
}
public class Demo1 {

	public static void main(String[] args) {
		
			//A a = new A();
			B b = new B();
	}
}
// as soon as we create a child class obj ,it automatically call the super default constructor















class Emp
{
		// decal all common properties 
		Emp(){
			// id , name ,age 
		}
}
class Clerk extends Emp
{
}
class Dev extends Emp
{
}