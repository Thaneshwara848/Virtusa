package comB;
class Car
{
	final int speed= 200 ;//we can not re assign the values 
	final void gare() {
		System.out.println("5 gare car");
	}
}
class Varna  extends Car
{
	int model=2017;
}
class BMW  extends Car
{
	int model=2020
//	@Override
//	void gare() {					if methos is fianl we cant over ride 
//		System.out.println("Auto gare car");
//	}
}
public class Demo1 {
	public static void main(String[] args) {
		Car c = new Car();
		System.out.println(c.speed);
		//c.speed=2000;			we can not re assign the values 		
		//System.out.println(c.speed);

		Varna v = new Varna();
		v.gare();
		
		BMW b = new BMW();
		b.gare();
	}
}
