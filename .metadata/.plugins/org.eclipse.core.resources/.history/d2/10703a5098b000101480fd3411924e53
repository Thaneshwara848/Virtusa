package PolyDemo;
class Emp
{
	void draw(){
		System.out.println("you can draw what ever you want ");
	}
	void draw(int r){
		System.out.println("Area of Cicle : "+ (2 *2.5 *r * r ));
	}
	void draw(int l,int b){
		System.out.println("Rectazngle :"+ (l *b));
	}

}
public class Demo1 {
	public static void main(String[] args) {

		Emp e = new Emp();
		e.draw();
	}
}
