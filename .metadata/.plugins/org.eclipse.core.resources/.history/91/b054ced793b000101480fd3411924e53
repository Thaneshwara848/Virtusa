package comC;
class Parent
{
	int x =1000;

}
class Child extends Parent
{
	int a =100;
	public void display(int a) {
		System.out.println(a);				// local variable 
		System.out.println(this.a);			// current class 
		System.out.println(a);		// parent class 
	}

}
public class Demo1 {

	public static void main(String[] args) {
		
		Child c = new Child();
		c.display(10);
		
	}

}
